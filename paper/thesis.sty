  \endgroup}

% Caption for continued figures, no TOC entry and no counter change 
% CCB 4,1994

\def\captioncont{\refstepcounter\@captype \@dblarg{\@captioncont \@captype} }

\long\def\@captioncont#1[#2]#3{\begingroup
    \@parboxrestore
    \normalsize
    \@makecaption{\csname fnum@#1\endcsname}{\ignorespaces #3}
    \par
  \endgroup}

