  ext@#1\endcsname}{#1}{\protect \hspace*{-0.5em} \numberline{\hfill \csname
  the#1\endcsname}{\ignorespaces \hspace{0.5em} #2}}\begingroup
    \@parboxrestore
    \normalsize
